{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "Kestrel": {
    "Endpoints": {
      "Http": {
        "Url": "http://*:5000"
      }
    },
    "Limits": {
      "MaxConcurrentConnections": 1000,
      "MaxConcurrentUpgradedConnections": 1000,
      "MaxRequestBodySize": null
    }
  },
  "AppSettings": {
    "SampleConfig": "TEST"
  },
  "ConnectionStrings": {
    "SampleContext": "Data Source=database.db;Cache=Shared"
  },
  "AllowedHosts": "*"
}